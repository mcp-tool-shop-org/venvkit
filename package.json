    "diagnostics",
    "health-check",
    "windows",
    "ml",
    "pytorch",
    "environment"
  ],
  "license": "MIT",
  "homepage": "https://github.com/mcp-tool-shop-org/venvkit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcp-tool-shop-org/venvkit.git"
